{
  "extends": ["plugin:cypress/recommended", "airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "es2023": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": "off",
    "no-unused-vars": "off"
  },
  "overrides": [
    {
      "files": "**/*.ts",
      "parser": "@typescript-eslint/parser",
      "plugins": ["@typescript-eslint"]
    }
  ]
}
